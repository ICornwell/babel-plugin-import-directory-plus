{
  "name": "babel-plugin-import-directory-plus",
  "version": "1.1.0",
  "description": "Are you sick and tired of writing an `index.js` file, just to import/export all the other files in a directory?\n\nDon't seek more :)\n\nJust use `babel-plugin-import-directory-plus` for that!",
  "main": "lib/index.js",
  "scripts": {
    "readme": "node-readme",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:coverage:report": "vitest --coverage --reporter=html",
    "build": "babel -d ./lib ./src",
    "prepublish": "babel -d ./lib ./src --minified"
  },
  "dependencies": {
    "@babel/template": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/ui": "^3.1.4",
    "node-readme": "^0.1.9",
    "vitest": "^3.1.4"
  },
  "repository": "git@github.com:icornwell/babel-plugin-import-directory-plus.git",
  "author": "IC <ian@smallwalrus.com> + copilt from work by Anmo <btavares26@gmail.com>",
  "license": "MIT"
}
